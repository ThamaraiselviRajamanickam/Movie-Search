{
    "name": "My Extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "web_accessible_resources": [
      "src/assets/styles.css",
      "src/assets/locales/en/translation.json"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.bundle.js"]
      }
    ],
    "permissions": ["<all_urls>"]
  }
  